<?xml version="1.0"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-2level - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="gov.cms" level="INFO">
        <appender-ref ref="STDOUT"/>
    </logger>
    <logger name="CLIENT-LOG" level="INFO">
        <appender-ref ref="STDOUT"/>
    </logger>
</configuration>
